<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TerminarRegistroActivity">

    <LinearLayout
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="296dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent">

        <EditText
            android:id="@+id/emailParaRegistroEditText"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:backgroundTint="#000000"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Email" />

        <EditText
            android:id="@+id/contrase単a1EditText"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:backgroundTint="#000000"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Contrase単a" />

        <EditText
            android:id="@+id/contrase単a2EditText"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:backgroundTint="#000000"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="ConfirmarContrase単a" />

        <Button
            android:id="@+id/btnTerminrRegistro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#B4241C"
            android:text="Registarse" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>